{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["@types/react"],
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/CustomTabBar.jsx"
  ]
}

